var GlideVar=window.Glide,glideConfig={type:"slider",startAt:0,perView:1,swipeThreshold:50,dragThreshold:50,keyboard:!1,focusAt:"center",rewind:!1},glide=new GlideVar(".glide",glideConfig);window.glideObj=glide;var titleElt=document.querySelector("#title"),dataCardTitles=titleElt.getAttribute("data-card-titles");glide.on("move.after",function(){var a=glide.index;titleElt.textContent=JSON.parse(dataCardTitles)[a]});glide.mount();
document.querySelectorAll(".glide__arrow").forEach(function(a){return a.addEventListener("click",function(a){a.preventDefault();a.stopPropagation()})});var clickElt=document.querySelector("#click-tracking-url");clickElt.addEventListener("click",function(a){a.preventDefault();a=glide.index;window.open(JSON.parse(clickElt.getAttribute("data-click-urls"))[a])});